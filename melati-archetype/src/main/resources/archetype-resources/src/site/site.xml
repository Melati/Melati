#set ($dollar = "$")
<?xml version="1.0" encoding="ISO-8859-1"?>
<project name="${dollar}{project.name}">
  <skin>
    <groupId>org.melati</groupId>
    <artifactId>melati-skin</artifactId>
    <version>1.0</version>
  </skin>
  <bannerLeft>
    <name>${artifactId}</name>
    <src>images/logo.png</src> 
    <href>http://paneris.net/</href>
  </bannerLeft>

  <bannerRight>
    <name>Melati</name>
    <src>http://www.melati.org/images/logo.jpg</src> 
    <href>http://www.melati.org/</href>
  </bannerRight>

  <body>
    <links>
      <item name="Melati"                href="http://www.melati.org" />
      <item name="PanEris.net"           href="http://www.paneris.net" />
    </links>
    <menu name="${dollar}{project.name}">
        <item name="Home" href="index.html"/>
        <item name="Download" href="Download.html"/>
        <item name="Usage" href="Usage.html"/>
    </menu>
    ${dollar}{reports}
  </body>
</project>
