#set $Response.ContentType = "text/html"
<html>
<head>
<title>Website Admin Suite</title>
</head>

<frameset frameborder=1 framespacing=3 border=3 cols="40%,60%">
  <frameset frameborder="0" framespacing="0" border="0" rows="45,*">
    <frame src="$admin.PrimarySelectURL($table)" marginwidth="0" marginheight="5" noresize="noresize" name="admin_primaryselect" border="0" scrolling="no">
    <frameset frameborder="0" framespacing="0" border="0" cols="*,36">
        <frame src="$admin.StaticURL/blank.html" marginwidth="-0" marginheight="-0" noresize="noresize" name="admin_selection" border="0" scrolling="auto">
        <frame src="$admin.NavigationURL($table)" marginwidth="4" marginheight="5" noresize="noresize" name="admin_navigation" border="0" scrolling="no">
    </frameset>
  </frameset>
  <frame src="$admin.StaticURL/blank.html" marginwidth="5" marginheight="-0" noresize="noresize" name="admin_record" border="0" scrolling="auto">
</frameset>

<noframes>
</noframes>

</html>
